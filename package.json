{
  "type": "module",
  "name": "site123",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tsc & node src/www.js",
    "dev": "npm run tsc & concurrently --kill-others \"npm run tsc:watch\" \"npm run watch-changes\"",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "watch-changes": "nodemon src/www.js"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "ip": "^1.1.8",
    "jsonwebtoken": "^8.5.1",
    "music-metadata": "^7.11.3",
    "puppeteer": "^13.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.12.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
