{
  "type": "module",
  "name": "site123",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tsc && node src/www.js",
    "dev": "npm run tsc && concurrently --kill-others \"npm run tsc:watch\" \"npm run watch-changes\"",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "watch-changes": "nodemon src/www.js"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "dotenv-flow": "^3.2.0",
    "express": "~4.18.2",
    "express-validator": "^6.14.3",
    "ip": "^1.1.8",
    "jsonwebtoken": "^8.5.1",
    "music-metadata": "^7.13.3",
    "puppeteer": "^13.0.1"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.15",
    "@types/ip": "^1.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
